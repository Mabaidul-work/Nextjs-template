{
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "arrowParens": "always",
    "semi": true,
    "jsxSingleQuote": false,
    "jsxBracketSameLine": false,
  
    "singleAttributePerLine": true,
    "useTabs": false,
    "overrides": [
      {
        "files": [".prettierrc", "*.json"],
        "options": { "parser": "json" }
      },
      {
        "files": ["*.scss"],
        "options": {
          "parser": "scss"
        }
      }
    ]
  }
  